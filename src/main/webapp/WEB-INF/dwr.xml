<!DOCTYPE dwr PUBLIC
    "-//GetAhead Limited//DTD Direct Web Remoting 3.0//EN"
    "http://directwebremoting.org/schema/dwr30.dtd">
    
<dwr>
	<allow>
		<!-- Initialization -->
		<create creator="new" javascript="Category">
			<param name="class" value="com.fanjavaid.ic.controller.CategoryController" />
		</create>
		
		<create creator="new" javascript="Item">
			<param name="class" value="com.fanjavaid.ic.controller.ItemController" />
		</create>
		
		<!-- Bean Converter -->
		<convert converter="bean" match="com.fanjavaid.ic.model.Category"/>
		<convert converter="bean" match="com.fanjavaid.ic.model.Item"/>
	</allow>
</dwr>